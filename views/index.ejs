<h1>MongoDB Contact App!</h1>

<h2>Halo <%= name %></h2>

<h2>DAFTAR PARTY SABLENG</h2>

<!-- * control flow EJS -->
<% if (mahasiswa.length < 1) { %>
<p>Data Mahasiswa masih kosong.</p>
<% } else { %>
<!-- * looping EJS -->
<% mahasiswa.forEach(map => { %>
<ul>
  <li>Nama: <%= map.name %></li>
  <li>Fetish: <%= map.fetish %></li>
</ul>
<% }) %>
<!--  -->
<% } %>
